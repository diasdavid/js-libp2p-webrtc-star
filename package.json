{
  "name": "libp2p-webrtc-star",
  "version": "0.8.8",
  "description": "libp2p WebRTC transport that includes a discovery mechanism provided by the signalling-star",
  "main": "src/index.js",
  "bin": {
    "star-sig": "src/sig-server/bin.js",
    "star-signal": "src/sig-server/bin.js"
  },
  "browser": {
    "wrtc": false
  },
  "scripts": {
    "star-signal": "node src/sig-server/bin.js",
    "lint": "gulp lint",
    "build": "gulp build",
    "test": "gulp test --webworker=false",
    "test:node": "gulp test:node",
    "test:browser": "gulp test:browser --webworker=false",
    "test:dns": "WEBRTC_STAR_REMOTE_SIGNAL_DNS=1 gulp test --webworker=false",
    "test:ip": "WEBRTC_STAR_REMOTE_SIGNAL_IP=1 gulp test --webworker=false",
    "release": "gulp release --webworker=false",
    "release-minor": "gulp release --type minor --webworker=false",
    "release-major": "gulp release --type major --webworker=false",
    "coverage": "gulp coverage",
    "coverage-publish": "aegir-coverage publish"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libp2p/js-libp2p-webrtc-star.git"
  },
  "keywords": [
    "IPFS",
    "libp2p"
  ],
  "author": "David Dias <daviddias@ipfs.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/libp2p/js-libp2p-webrtc-star/issues"
  },
  "homepage": "https://github.com/libp2p/js-libp2p-webrtc-star#readme",
  "devDependencies": {
    "aegir": "^11.0.0",
    "bl": "^1.2.0",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "lodash": "^4.17.4",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "async": "^2.1.5",
    "webrtcsupport": "^2.2.0",
    "debug": "^2.6.1",
    "detect-node": "^2.0.3",
    "hapi": "^16.1.0",
    "interface-connection": "~0.3.1",
    "mafmt": "^2.1.6",
    "minimist": "^1.2.0",
    "multiaddr": "^2.2.1",
    "once": "^1.4.0",
    "peer-id": "~0.8.2",
    "peer-info": "~0.8.3",
    "pull-stream": "^3.5.0",
    "simple-peer": "^7.0.0",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "stream-to-pull-stream": "^1.7.2",
    "wrtc": "0.0.61"
  },
  "contributors": [
    "David Dias <daviddias.p@gmail.com>",
    "Friedel Ziegelmayer <dignifiedquire@gmail.com>",
    "Juan Benet <juan@benet.ai>",
    "Michael Fakhry <fakhrimichael@live.com>",
    "Richard Littauer <richard.littauer@gmail.com>",
    "greenkeeperio-bot <support@greenkeeper.io>",
    "interfect <interfect@gmail.com>",
    "ᴠɪᴄᴛᴏʀ ʙᴊᴇʟᴋʜᴏʟᴍ <victorbjelkholm@gmail.com>"
  ]
}